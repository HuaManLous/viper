<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 1 级联赋值 -->
    <bean id="emp" class="com.summary.hml.viper.spring_study.ioc.bean.Emp">
        <property name="love">
            <list>
                <value>读书</value>
                <value>听歌</value>
                <value>打球</value>
            </list>
        </property>
          <property name="name" value="张三"></property>
          <property name="age" value="18"></property>
          <property name="dept" ref="dept"></property>
    </bean>

    <bean id="dept" class="com.summary.hml.viper.spring_study.ioc.bean.Dept">
        <property name="name" value="1.级联赋值name"></property>
        <property name="address" value="1.级联赋值address"></property>
    </bean>

  <!--   =======================================================================================================   -->

    <!-- 2 级联赋值 -->
    <bean id="emp2" class="com.summary.hml.viper.spring_study.ioc.bean.Emp">
        <property name="dept" ref="dept2"></property>
        <property name="dept.name" value="2.级联赋值.财务部门"></property>
    </bean>

    <bean id="dept2" class="com.summary.hml.viper.spring_study.ioc.bean.Dept"></bean>
</beans>
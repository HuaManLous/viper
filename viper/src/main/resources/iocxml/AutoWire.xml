<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 练习spring 自动装配
        autowire :
            byName: 按照属性名称自动装配，ID对应的名称要和属性名称一致
            byType: 按照属性类型自动装配，有多个类型时会报错
    -->
    <bean id="autoWireA" class="com.summary.hml.viper.spring_study.ioc.autowire.AutoWireA" autowire="byName"/>

    <bean id="autoWireB" class="com.summary.hml.viper.spring_study.ioc.autowire.AutoWireB">
        <property name="name" value="自动装配"/>
    </bean>

    <bean id="autoWireB2" class="com.summary.hml.viper.spring_study.ioc.autowire.AutoWireB">
        <property name="name" value="自动装配"/>
    </bean>

</beans>